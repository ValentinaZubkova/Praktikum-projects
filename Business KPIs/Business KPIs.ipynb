   "metadata": {},
   "outputs": [],
   "source": [
    "#Посчитаем количество новых покупателей за каждый месяц\n",
    "cohort_orders_sizes = first_orders.groupby('first_order_month').agg({'Uid': 'nunique'}).reset_index()\n",
    "cohort_orders_sizes.columns = ['first_order_month', 'n_buyers']"
   ]
  },
